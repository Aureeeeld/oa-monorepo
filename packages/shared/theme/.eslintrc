{
  "extends": ["airbnb-typescript-prettier", "prettier", "prettier/react"],
  "plugins": ["react", "react-hooks", "prettier", "import", "jsx-a11y"],
  "parser": "@typescript-eslint/parser",
  "rules": {},
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["#pages", "./src/pages"],
          ["#store", "./src/store"]
        ],
        "extensions": [".ts", ".tsx", ".js", ".jsx", ".json"]
      }
    }
  }
}
