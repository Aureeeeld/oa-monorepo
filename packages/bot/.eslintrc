{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-typescript-prettier", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off"
  }
}
